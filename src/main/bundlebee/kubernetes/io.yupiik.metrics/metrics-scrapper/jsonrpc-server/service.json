{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "metrics-scrapper-jsonrpc-server",
    "labels": {
      "app": "metrics-scrapper-jsonrpc-server",
      "deploy.by":"{{user.name:-unknown}}"
    }
  },
  "spec":{
    "type": "NodePort",
    "selector": {
      "app": "metrics-scrapper-jsonrpc-server"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 8080,
        "targetPort": 8080
      }
    ]
  }
}